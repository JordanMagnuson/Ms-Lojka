\u000a});\u000a\u000avar sndArray = Array(sndKey01, sndKey02, sndKey03, sndKey04, sndKey05,sndKey06, sndKey07, sndKey08);\u000avar sndRandom = sndKey01;\u000avar minVolume = 0.1;\u000avar maxVolume = 0.6;\u000avar volume = minVolume;\u000a\u000awindow.playRandomKeystrokeSound = function(volume) {\u000a	sndRandom = sndArray[Math.floor(Math.random() * sndArray.length)];\u000a	if (!volume) {\u000a		volume = Math.random() * maxVolume;\u000a	}\u000a	if (volume < minVolume) {\u000a		volume = minVolume;\u000a	}\u000a	sndRandom.volume(volume);\u000a	sndRandom.play();\u000a};\u000a\u000a/** Glitch stuff **/\u000awindow.playRandomGlitchSound = function(length) {\u000a	try\u000a	{\u000a		var volume = 0.5;\u000a		/**\u000a		var duration = sndGlitch.duration();\u000a		var position = Math.random() * duration;\u000a		sndGlitch.seek(position);\u000a		**/\u000a		sndGlitch.volume(volume);\u000a		sndGlitch.play();\u000a		setTimeout(function() { \u000a			sndGlitch.pause();\u000a		}, length);		\u000a	}\u000a	catch(e) {\u000a	}\u000a};\u000a\u000awindow.neonSign = function(){\u000a    var randSeconds = Math.floor(Math.random()*2000)+0;\u000a    var randDelay = randSeconds;\u000a    var speed=300;\u000a    var $toAnimate=$('body.neon-flicker');\u000a    $toAnimate.delay(randDelay).animate({'opacity':0.3},speed,function() {\u000a        $toAnimate.animate({'opacity':1},speed,neonSign() );\u000a		if (Math.random() > 0.5)\u000a		{\u000a			var volume = Math.random() * 0.6;\u000a		}\u000a    });\u000a};\u000a\u000a/** Whisper sounds in 'between' passages (and end sequence). **/\u000apostdisplay.handleWhisperSounds = function() {\u000a	if (tags().contains('endsequence')) {\u000a		return;\u000a	}\u000a	else {\u000a		if (tags().contains('between'))\u000a		{\u000a			sndWhispers.play();\u000a		}\u000a		else {\u000a			sndWhispers.stop();\u000a		}\u000a	}\u000a};\u000a\u000apostdisplay.handleLinkClicks = function() {\u000a	if (State.passage != 'Warning')\u000a	{\u000a		/** Play keystroke sound on link clicks. **/\u000a		$('a').click(function() {\u000a			playRandomKeystrokeSound(0.7);\u000a		});\u000a	}\u000a};
