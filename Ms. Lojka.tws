V<div class="actor"><div id="lojka"></div></div>\u000a<<script>>\u000apostdisplay.sceneSetup = function(sceneSetup) {\u000a    var t = theaterJS({\u000a        minSpeed: { erase: 30, type: 50 },\u000a        maxSpeed: { erase: 100, type: 300 }\u000a    });\u000a    t.on("type:start, erase:start", function() {\u000a        var e = t.getCurrentActor();\u000a        e.$element.classList.add("is-typing")\u000a    }).on("type:end, erase:end", function() {\u000a        var e = t.getCurrentActor();\u000a        e.$element.classList.remove("is-typing");\u000a    }), t.addActor("lojka", {\u000a        accuracy: .79,\u000a        speed: .7\u000a    });\u000a	do {\u000a		var leftArray = Array('I am ', 'You are ', 'We are ');\u000a		shuffle(leftArray);\u000a		var leftMistake = leftArray.pop();\u000a		var leftReplace = leftArray.pop();\u000a	}\u000a	while (leftReplace == state.active.variables.leftReplace);\u000a	state.active.variables.leftReplace = leftReplace;\u000a	do {\u000a		var rightArray = Array('I must ', 'you must ', 'we must ');\u000a		shuffle(rightArray);\u000a		var rightMistake = rightArray.pop();\u000a		var rightReplace = rightArray.pop();\u000a	}\u000a	while (rightReplace == state.active.variables.rightReplace);\u000a	state.active.variables.rightReplace = rightReplace;\u000a	t.addScene(1000, "lojka: " + leftMistake, -leftMistake.length, leftReplace + "Ms. Lojka,", 200, " my precious,", 300, " and such has always been the price " + rightMistake, -rightMistake.length, rightReplace + "pay for living.", 1000); \u000a	t.addScene("lojka: "); \u000a	t.addScene(function (done) {\u000a		State.play('Lojka8');\u000a		done();\u000a	  });\u000a};\u000afunction shuffle(a) {\u000a    var j, x, i;\u000a    for (i = a.length; i; i -= 1) {\u000a        j = Math.floor(Math.random() * i);\u000a        x = a[i - 1];\u000a        a[i - 1] = a[j];\u000a        a[j] = x;\u000a    }\u000a}\u000a<</script>>
