    //callback for every typed string\u000a        onStringTyped: function() {\u000a			//console.log('in onStringTyped');\u000a		},\u000a        // callback for reset\u000a        resetCallback: function() {\u000a			//console.log('in resetCallback');\u000a		}\u000a    };\u000a\u000a\u000a}(window.jQuery);
